#include<stdio.h>
#include<string.h>

int main(void)
{
    char Sentence[100];
    int i = 0;
    int Space, k;

    gets(Sentence);
    while(Sentence[i] != '\0')
        i++;
    Space = i;
   
    for(; i >= 0; i--)    //for循环实习颠倒
    {
        if(Sentence[i] == ' ')
        {
            for(k = i + 1; k <= Space; k ++)
                putchar(Sentence[k]);
            Space = i;
        }
    }
    //剩一个首单词
    for(k = i + 1; k <= Space; k++)
        putchar(Sentence[k]);

    return 0;
}
